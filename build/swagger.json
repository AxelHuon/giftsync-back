	"paths": {
		"/auth/signup": {
			"post": {
				"operationId": "RegisterUser",
				"responses": {
					"200": {
						"description": "User successfully registered",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"code": {
											"type": "string"
										},
										"message": {
											"type": "string"
										}
									},
									"required": [
										"code",
										"message"
									],
									"type": "object"
								}
							}
						},
						"headers": {
							"RegisterUserResponse": {
								"schema": {
									"$ref": "#/components/schemas/RegisterUserResponse"
								}
							}
						}
					},
					"400": {
						"description": "Email is already associated with an account"
					},
					"500": {
						"description": "Error in registering user"
					}
				},
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/RegisterUserRequest"
							}
						}
					}
				}
			}
		},
		"/auth/signin": {
			"post": {
				"operationId": "SignInUser",
				"responses": {
					"200": {
						"description": "User successfully signed in",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"properties": {
												"refreshToken": {},
												"accessToken": {},
												"email": {},
												"lastName": {},
												"firstName": {},
												"id": {},
												"code": {
													"type": "string"
												},
												"message": {
													"type": "string"
												}
											},
											"required": [
												"code",
												"message"
											],
											"type": "object"
										},
										{
											"properties": {
												"code": {},
												"message": {},
												"refreshToken": {
													"type": "string"
												},
												"accessToken": {},
												"email": {
													"type": "string"
												},
												"lastName": {
													"type": "string"
												},
												"firstName": {
													"type": "string"
												},
												"id": {
													"type": "string"
												}
											},
											"required": [
												"refreshToken",
												"accessToken",
												"email",
												"lastName",
												"firstName",
												"id"
											],
											"type": "object"
										}
									]
								}
							}
						},
						"headers": {
							"SignInUserResponse": {
								"schema": {
									"$ref": "#/components/schemas/SignInUserResponse"
								}
							}
						}
					},
					"400": {
						"description": "Email and password are required"
					},
					"401": {
						"description": "Incorrect email and password combination"
					},
					"404": {
						"description": "User not found"
					},
					"500": {
						"description": "Error in signIn user"
					}
				},
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SignInUserRequest"
							}
						}
					}
				}
			}
		},
