			"post": {
				"operationId": "RefreshToken",
				"responses": {
					"200": {
						"description": "Tokens successfully refreshed",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"properties": {
												"refreshToken": {},
												"accessToken": {},
												"code": {
													"type": "string"
												},
												"message": {
													"type": "string"
												}
											},
											"required": [
												"code",
												"message"
											],
											"type": "object"
										},
										{
											"properties": {
												"code": {},
												"message": {},
